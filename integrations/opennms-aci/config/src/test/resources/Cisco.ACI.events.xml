<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
         <event>
                <uei>uei.opennms.org/cisco/aci/critical</uei>
                <event-label>Cisco ACI Critical Event: %parm[descr]% for affected: %parm[affected]% cause: %parm[cause]% - LC: %parm[lc]%</event-label>
                <descr>
                        Node ID: %nodeid%&lt;br&gt;
                        Host: %nodelabel%&lt;br&gt;
                        Interface: %interface% &lt;br&gt;
			Parms:  %parm[all]%
                </descr>
                <logmsg dest='logndisplay'>Cisco ACI Critical Event: %parm[code]% - %parm[descr]% for affected: %parm[affected]% cause: %parm[cause]% - LC: %parm[lc]%</logmsg>
                <severity>Critical</severity>
                <alarm-data reduction-key="%nodeid%:%parm[cause]%:%parm[affected]%" alarm-type="1" auto-clean="false">
                    <update-field field-name="severity" update-on-reduction="true"/>
                </alarm-data>
        </event>
