<events xmlns="http://xmlns.opennms.org/xsd/eventconf">
         <event>
                <uei>uei.opennms.org/cisco/aci/critical</uei>
                <event-label>Cisco ACI Critical Event: %parm[descr]% for object: %parm[dn]%</event-label>
                <descr>
                        Node ID: %nodeid%&lt;br&gt;
                        Host: %nodelabel%&lt;br&gt;
                        Interface: %interface% &lt;br&gt;
			                  Parms:  %parm[all]%
                </descr>
                <logmsg dest='logndisplay'>
                        OpenNMS received a Cisco ACI Critical Event: %parm[descr]% for object: %parm[dn]%
                </logmsg>
                <severity>Critical</severity>
                <alarm-data reduction-key="%nodeid%:%parm[cause]%:%parm[affected]%" alarm-type="3" auto-clean="false"/>
        </event>
